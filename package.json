{
  "name": "VeloadMonitor",
  "version": "0.0.7",
  "description": "where weekend warriors train",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "postinstall": "electron-builder install-app-deps",
    "pack": "electron-builder -w --dir",
    "dist": "electron-builder -w",
    "publish": "electron-builder -w -p always"
  },
  "repository": "https://github.com/spencermize/VeloadMonitor",
  "keywords": [],
  "build": {
    "appId": "org.veload.monitor",
    "extraFiles": [
      "**icon.png"
    ]
  },
  "publish": {
    "provider": "github"
  },
  "author": "Spencer Mize",
  "devDependencies": {
    "electron": "latest",
    "electron-rebuild": "latest",
    "electron-builder": "latest"
  },
  "dependencies": {
    "ant-plus": "0.0.23",
    "cors": "latest",
    "electron-store": "^8.0.0",
    "electron-updater": "^4.0.6",
    "express": "4.16.4",
    "spdy": "^4.0.0",
    "usb-detection": "latest"
  }
}
